package w1task;

public class Orderofvowels {
	public static void main(String[] args) {
		String s="hey are you checcking vowels order right";
		s=s.replace("bcdfghjklmnpqrstvwxyz","");
		String arr[]=s.split("");
		if(s!=null && s.matches("[a][e][i][o][u]"))
		{
			System.out.println("valid");
		}
		else
			System.out.println("not valid");
		//for(int i=0;i<s.length();i++)

	}

}
