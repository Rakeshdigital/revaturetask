package w1task;
import java.util.Arrays;

public class Palindrome {

	public static void main(String[] args) {
		int ar[]= {1,2,3,2,1} ;
		int length=ar.length;
		for(int i=0;i<ar.length;i++)
		{
		  int Start  =ar[i];
		  int end  =ar[--length];
		  
		  if(Start !=end)
		  {
			  System.out.print("not polindrome");
		  }
		  
		 //ar[i]=ar[i];
		  
		
        //System.out.println(ar[i]);
		}
	}

}
