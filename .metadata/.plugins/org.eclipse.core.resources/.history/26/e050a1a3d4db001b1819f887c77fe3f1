package w1task;

public class prime {

	public static void main(String[] args) {
		int ar[]= {2,3,4,5,6,7,8,9,10};
		for(int i=0;i<=ar.length;i++)
		{
		 int n=ar[i];
		 if(n>=2)
		 {
			if(n%2==0)
			{
				System.out.println("its not  prime num"+ar[i]);
			}
			else
				System.out.println("its prime"+ar[i]);
		 }
		}
	}
		
}
