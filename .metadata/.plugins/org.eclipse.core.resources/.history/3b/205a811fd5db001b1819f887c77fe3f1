package w1task;

public class prime {

	public static void main(String[] args) {
		int ar[]= {2,3,4,5,6,7,8,9,10};
		for(int i=0;i<=ar.length;i++)
		{
		 
		 if(ar[i]==2)
		 {
			 System.out.println("its prime"+ar[i]);
		 }
		 if(ar[i]>2)
		 {
			if(ar[i]%2==0)
			{
				System.out.println(ar[i]+ " - its not  prime num");
			}
			else
				System.out.println(ar[i]+ " - its  a prime number");
		 }
		}
	}
		
}
